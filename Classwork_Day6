#Cubes
range_start = int(float(input("Pick a number that will determine the beginning of a range")))
range_end = int(float(input("Pick a larger number that will determine the end of a range")))
make_list = list(range(range_start,range_end+1))

cubes = []
for num in make_list:
    single_cube=num**3
    cubes.append(single_cube)
    print(f"{num} cubed: {single_cube}")
print(f"All cubed is {cubes}")
